{
  "scopeName": "text.txxt.template",
  "name": "Txxt Template",
  "patterns": [
    {
      "match": "^\\d+\\.\\s+",
      "name": "markup.template.sequence.txxt"
    },
    {
      "match": ".*$",
      "name": "markup.template.content.txxt"
    }
  ],
  "injectionSelector": "L:text.txxt",
  "injectTo": ["text.txxt"],
  "embeddedLanguages": {
    "text.txxt": "text.txxt"
  },
  "include": "text.txxt"
}
