{
  "name": "Simple Test Grammar",
  "scopeName": "source.test",
  "patterns": [
    {
      "match": "\\b(function|class|const|let|var)\\b",
      "name": "keyword.control.test"
    },
    {
      "match": "\\b([A-Za-z_][A-Za-z0-9_]*)\\s*\\(",
      "captures": {
        "1": {
          "name": "entity.name.function.test"
        }
      }
    },
    {
      "match": "\\b([0-9]+)\\b",
      "name": "constant.numeric.test"
    },
    {
      "begin": "\"",
      "end": "\"",
      "name": "string.quoted.double.test",
      "patterns": [
        {
          "match": "\\\\.",
          "name": "constant.character.escape.test"
        }
      ]
    },
    {
      "begin": "'",
      "end": "'",
      "name": "string.quoted.single.test",
      "patterns": [
        {
          "match": "\\\\.",
          "name": "constant.character.escape.test"
        }
      ]
    },
    {
      "match": "//.*$",
      "name": "comment.line.double-slash.test"
    },
    {
      "begin": "/\\*",
      "end": "\\*/",
      "name": "comment.block.test"
    }
  ]
}
